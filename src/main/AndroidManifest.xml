<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Newapp"
        tools:targetApi="31">
        <activity
            android:name=".kartavya"
            android:exported="false" />
        <activity
            android:name=".profilepage"
            android:exported="false" />
        <activity
            android:name=".employerregister"
            android:exported="false"
            android:label="employerregistration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".jobseekerregistration"
            android:exported="false"
            android:label="jobseekerregistration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".rozgarmela"
            android:exported="false"
            android:label="rozgarmela"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".loginpage"
            android:exported="false"
            android:label="loginpage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>